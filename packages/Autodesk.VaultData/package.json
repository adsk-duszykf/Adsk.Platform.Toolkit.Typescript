{
  "name": "@adsk-platform/vaultdata",
  "version": "0.2.1",
  "description": "Autodesk Vault Data SDK for TypeScript",
  "type": "module",
  "scripts": {
    "build": "rimraf ./_dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "publish": "bun publish --production",
    "lint": "bun biome check --write",
    "test": "bun test"
  },
  "exports": {
    ".": {
      "import": "./_dist/index.js"
    },
    "./models": {
      "import": "./_dist/generatedCode/models/index.js"
    }
  },
  "files": [
    "./_dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adsk-duszykf/Adsk.Platform.Toolkit.Typescript",
    "directory": "packages/Autodesk.VaultData"
  },
  "homepage": "https://github.com/adsk-duszykf/Adsk.Platform.Toolkit.Typescript/tree/main/packages/Autodesk.VaultData",
  "bugs": {
    "url": "https://github.com/adsk-duszykf/Adsk.Platform.Toolkit.Typescript/issues"
  },
  "keywords": [
    "autodesk",
    "vault",
    "vault-data",
    "sdk",
    "typescript",
    "api",
    "kiota"
  ],
  "author": "duszykf",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@microsoft/kiota-bundle": "catalog:"
  },
  "devDependencies": {
    "bun-fetch-mock": "workspace:*",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2"
  }
}
